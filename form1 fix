        // --- TAB 3D VISUALIZATION ---
        private async void Initialize3DTab()
        {
            Panel panel3D = new Panel { Dock = DockStyle.Fill, BackColor = Color.Black };
            // Inisialisasi WebView2
            webView3D = new WebView2
            {
                Dock = DockStyle.Fill
            };
            // Pastikan WebView2 Core terinisialisasi
            try
            {
                await webView3D.EnsureCoreWebView2Async(null);
            }
            catch (Exception ex)
            {
                MessageBox.Show("Gagal memuat WebView2 Runtime. Pastikan Microsoft Edge WebView2 Runtime terinstall.\nError: " + ex.Message);
                return;
            }
            panel3D.Controls.Add(webView3D);
            tab3DVisualization.Controls.Add(panel3D);
            // Memuat File GLB
            Load3DModel();
        }

        private async void Load3DModel()
        {
            // 1. Siapkan WebView2
            if (webView3D != null && webView3D.CoreWebView2 == null)
            {
                await webView3D.EnsureCoreWebView2Async();
            }

            // 2. Ambil File
            string currentDir = AppDomain.CurrentDomain.BaseDirectory;
            string glbPath = Path.Combine(currentDir, "cooling_tower.glb");

            if (!File.Exists(glbPath))
            {
                MessageBox.Show("File cooling_tower.glb tidak ditemukan!");
                return;
            }

            // 3. Cek ukuran file (batas aman sekitar 1.5MB agar NavigateToString tidak error)
            FileInfo fileInfo = new FileInfo(glbPath);
            long fileSizeInBytes = fileInfo.Length;
            double fileSizeInMB = fileSizeInBytes / (1024.0 * 1024.0);

            if (fileSizeInMB > 1.5) // Misalnya, batas 1.5MB
            {
                MessageBox.Show($"File cooling_tower.glb terlalu besar ({fileSizeInMB:F2} MB). " +
                                "Ukuran file terlalu besar untuk dimuat dalam aplikasi saat ini. " +
                                "Harap gunakan file GLB yang lebih kecil.");
                return;
            }

            try
            {
                // 4. Convert ke Base64
                byte[] modelBytes = File.ReadAllBytes(glbPath);
                string base64Model = Convert.ToBase64String(modelBytes);
                string modelUri = $"data:model/gltf-binary;base64,{base64Model}";

                // 5. HTML sederhana untuk model-viewer
                string htmlContent = $@"
<!DOCTYPE html>
<html>
<head>
    <title>3D Viewer</title>
    <meta charset=""utf-8"">
    <script type=""module"" src=""https://ajax.googleapis.com/ajax/libs/model-viewer/3.4.0/model-viewer.min.js""></script>
    <style>
        body {{ margin: 0; background-color: #333333; overflow: hidden; }}
        model-viewer {{ width: 100vw; height: 100vh; }}
    </style>
</head>
<body>
    <model-viewer 
        src=""{modelUri}"" 
        alt=""Cooling Tower""
        environment-image=""https://modelviewer.dev/shared-assets/environments/aircraft_workshop_01_1k.hdr""
        shadow-intensity=""1""
        exposure=""1""
        tone-mapping=""aces""
        camera-controls 
        auto-rotate
        camera-orbit=""45deg 55deg 105%"" 
        min-camera-orbit=""auto auto 5%"">
    </model-viewer>
</body>
</html>";

                // 6. Navigate ke string HTML
                webView3D.NavigateToString(htmlContent);
            }
            catch (Exception ex)
            {
                MessageBox.Show($"Gagal memuat 3D Model: {ex.Message}");
            }
        }

        // --- TAB PHYSICS FORMULAS ---
        private void InitializeFormulaTab()
        {
            // 1. Container Utama: TableLayoutPanel
            // Kita bagi layar menjadi 2 baris dengan tinggi masing-masing 50% layar
            TableLayoutPanel mainTable = new TableLayoutPanel
            {
                Dock = DockStyle.Fill,
                ColumnCount = 1,
                RowCount = 2,
                Padding = new Padding(10),
                BackColor = Color.WhiteSmoke // Warna background sedikit abu agar kontras
            };

            // Atur agar kedua gambar berbagi ruang layar secara adil (50% - 50%)
            mainTable.ColumnStyles.Add(new ColumnStyle(SizeType.Percent, 100F));
            mainTable.RowStyles.Add(new RowStyle(SizeType.Percent, 50F)); // Baris 1: 50% tinggi
            mainTable.RowStyles.Add(new RowStyle(SizeType.Percent, 50F)); // Baris 2: 50% tinggi

            // 2. Tambahkan Bagian Input Formulas (Baris 0)
            AddFormulaSection(mainTable, "Rumus Input Sensor", "input_formulas.png", 0);

            // 3. Tambahkan Bagian Simulation Formulas (Baris 1)
            AddFormulaSection(mainTable, "Rumus Simulasi Program", "simulation_formulas.png", 1);

            // Tambahkan ke Tab
            tabFormulas.Controls.Add(mainTable);
        }

        // Helper Method: Untuk membuat section gambar agar kode lebih rapi & reusable
        private void AddFormulaSection(TableLayoutPanel parentTable, string titleText, string imageName, int rowIndex)
        {
            // Panel container untuk setiap section
            Panel container = new Panel
            {
                Dock = DockStyle.Fill,
                Margin = new Padding(5),
                BackColor = Color.White,
                BorderStyle = BorderStyle.FixedSingle // Memberi bingkai tipis agar rapi
            };

            // 1. Label Judul
            Label lblTitle = new Label
            {
                Text = titleText,
                Dock = DockStyle.Top,
                Height = 35,
                Font = new Font("Segoe UI", 11F, FontStyle.Bold),
                TextAlign = ContentAlignment.MiddleCenter,
                BackColor = Color.FromArgb(240, 240, 240), // Warna header soft
                ForeColor = Color.Black
            };

            // 2. PictureBox (Logika Anti-Crop)
            PictureBox pb = new PictureBox
            {
                Dock = DockStyle.Fill,              // Mengisi sisa ruang di bawah label
                SizeMode = PictureBoxSizeMode.Zoom, // KUNCI: Gambar di-scale agar muat (Fit) tanpa crop
                BackColor = Color.White,            // Background putih jika rasio gambar beda dengan panel
                Padding = new Padding(5)            // Memberi jarak sedikit dari bingkai
            };

            // Load Gambar dengan Error Handling
            string imagePath = Path.Combine(AppDomain.CurrentDomain.BaseDirectory, imageName);
            if (File.Exists(imagePath))
            {
                try
                {
                    // Menggunakan FileStream agar file tidak terkunci (optional, tapi good practice)
                    using (var fs = new FileStream(imagePath, FileMode.Open, FileAccess.Read))
                    {
                        pb.Image = Image.FromStream(fs);
                    }
                }
                catch (Exception ex)
                {
                    SetErrorImage(pb, "Gagal memuat gambar: " + ex.Message);
                }
            }
            else
            {
                SetErrorImage(pb, $"File '{imageName}' tidak ditemukan di folder Debug/Release!");
            }

            // Masukkan kontrol ke container
            // Urutan Add penting: Label dulu (Top), baru PictureBox (Fill)
            container.Controls.Add(pb);
            container.Controls.Add(lblTitle);

            // Masukkan container ke TableLayoutPanel utama
            parentTable.Controls.Add(container, 0, rowIndex);
        }

        // Helper kecil untuk tampilan jika gambar error/hilang
        private void SetErrorImage(PictureBox pb, string msg)
        {
            pb.Image = null;
            pb.BackColor = Color.MistyRose; // Warna merah muda tanda error

            // Buat label error di tengah PictureBox
            Label lblError = new Label
            {
                Text = "‚ö†Ô∏è " + msg,
                AutoSize = false,
                Dock = DockStyle.Fill,
                TextAlign = ContentAlignment.MiddleCenter,
                ForeColor = Color.Red,
                Font = new Font("Segoe UI", 9F, FontStyle.Italic)
            };
            pb.Controls.Add(lblError);
        }

